# API Configuration - Backend Server
REACT_APP_API_URL=http://localhost:3000
REACT_APP_API_TIMEOUT=10000

# Authentication Configuration
REACT_APP_AUTH_TOKEN_NAME=access_token
REACT_APP_JWT_SECRET=roomoree_dev_secret_2024
REACT_APP_TOKEN_EXPIRY=24h

# Application Settings
REACT_APP_APP_NAME=Roomoree
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true

# SEO & Meta Information
REACT_APP_SITE_URL=http://localhost:3000
REACT_APP_SITE_DESCRIPTION=Find your perfect stay with Roomoree - Property Rental Platform
REACT_APP_SITE_KEYWORDS=accommodation,booking,travel,rooms,apartments,rental,property

# Feature Flags
REACT_APP_ENABLE_SOCIAL_LOGIN=false
REACT_APP_ENABLE_CHAT=false
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_ANALYTICS=false

# Security Settings
REACT_APP_ENABLE_HTTPS=false
REACT_APP_CORS_ORIGIN=http://localhost:3000

# Email Configuration (for frontend display purposes)
