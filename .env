REACT_APP_CONTACT_EMAIL=contact@roomoree.com

# Rate Limiting (frontend display)
REACT_APP_API_RATE_LIMIT=100
REACT_APP_UPLOAD_MAX_SIZE=10485760

# Currency & Localization
REACT_APP_DEFAULT_CURRENCY=USD
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_CURRENCIES=USD,EUR,GBP,CAD,AUD

# Development Tools
REACT_APP_SHOW_CONNECTION_STATUS=true
REACT_APP_SHOW_DEBUG_INFO=true
REACT_APP_LOG_API_CALLS=true

